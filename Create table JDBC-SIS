CRATE TABLE sis.subject(
idSubject VARCHAR(255) NOT NULL,
nameSuject VARCHAR(45) NOT NULL,
nameOfCredit int,
PRIMARY KEY(idSubject)
);
CREATE TABLE sis.admin(
user VARCHAR(255) NOT NULL,
password VARCHAR(25) NOT NULL,
PRIMARY KEY(user)
);
CRATE TABLE sis.user(
id VARCHAR(355) NOT NULL,
password VARCHAR(45) NOT NULL,
name VARCHAR(45),
PRIMARY KEY(idSubject)
);
CRATE TABLE sis.subjectregister(
iduser VARCHAR(255) NOT NULL,
numberOfCredit int NOT NULL,
idSubject VARCHAR(255) NOT NULL,
nameSubject VARCHAR(45) NOT NULL,
idst int NOT NULL AUTO_INCREMENT,
PRIMARY KEY(idst)
);
